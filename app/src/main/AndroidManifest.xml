<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jchuah.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".color"
            android:label="@string/title_activity_color" >
        </activity>
        <activity
            android:name=".lastActivity"
            android:label="@string/title_activity_last"
            android:parentActivityName=".color" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jchuah.myapplication.color" />
        </activity>
    </application>

</manifest>
